# Local node runtime
JARVIS_WORKSPACE=$HOME/.jarvis_local_node/workspace
JARVIS_MODEL=deepseek-r1:1.5b
JARVIS_FALLBACK_MODELS=gemma3:4b,deepseek-r1:7b
JARVIS_AUTONOMOUS_ENABLED=false
JARVIS_AUTONOMOUS_INTERVAL_SEC=60
JARVIS_OPERATOR_KEY=change_me

# Optional coordinator enrollment (consent-based)
HIVE_COORDINATOR_URL=https://your-coordinator.example.com
HIVE_API_KEY=replace_me
NODE_LABEL=$(scutil --get ComputerName)
